{
  "name": "single-spa-example-shared-styled-components",
  "version": "1.0.0",
  "description": "single-spa example that has styled-components as a shared dependency",
  "scripts": {
    "setup": "run-s install:*",
    "install:root-config": "yarn install --prefix root-config",
    "install:styleguide": "yarn install --prefix styleguide",
    "install:navbar": "yarn install --prefix navbar",
    "start": "run-p start:*",
    "start:root-config": "yarn --cwd root-config start",
    "start:styleguide": "yarn --cwd styleguide start",
    "start:navbar": "yarn --cwd navbar start"
  },
  "keywords": [
    "single-spa",
    "example",
    "styled-components"
  ],
  "author": "@filoxo",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
