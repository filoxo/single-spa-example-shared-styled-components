    "@babel/runtime": "^7.8.7",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^27.2.4",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.18.0",
    "eslint-config-important-stuff": "^1.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "jest-cli": "^27.2.4",
    "prettier": "^2.0.4",
    "pretty-quick": "^3.1.0",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-config-single-spa": "^5.1.1",
