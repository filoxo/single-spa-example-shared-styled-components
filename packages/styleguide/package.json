{
  "name": "@filoxo/styleguide",
  "scripts": {
    "start": "webpack serve --port 9001",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext js",
